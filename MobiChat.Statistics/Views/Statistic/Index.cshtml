@model TableModel
  
  
  <div class="form-group">
  <h6>From</h6>
  <select class="form-control selectpicker" id="selectTime">
    <option custom="hour">Last Hour</option>
    <option custom="day">Today</option>
    <option custom="yesterday">Yesterday</option>
    <option custom="week">This Week</option>
    <option custom="month">This Month</option>
  </select>
  
  <button type="button" class="btn btn-info" onclick="select()">Select</button>
                     
  </div>
<hr />
<hr />


<div class="container">
  <div class="row">
    <div class="col-md-4">

      <div class='col-sm-6'>
        <h6>From</h6>
        <div class="form-group">
          <div class='input-group date' id='datetimepicker1'>
            <input type='text' id="datefrom" class="form-control" />
            <span class="input-group-addon">
              <span class="glyphicon glyphicon-calendar"></span>
            </span>
          </div>
        </div>
      </div>
      <script type="text/javascript">
        $('#datetimepicker1').datetimepicker({
          format: 'YYYY-MM-DD HH:mm'
        });
      </script>

     
      <div class='col-sm-6'>
        <h6>To</h6>
        <div class="form-group">
          <div class='input-group date' id='datetimepicker2'>
            <input type='text' id="dateto" class="form-control" />
            <span class="input-group-addon">
              <span class="glyphicon glyphicon-calendar"></span>
            </span>
          </div>
        </div>
      </div>
      <script type="text/javascript">
            $(function () {
              $('#datetimepicker2').datetimepicker({
                format: 'YYYY-MM-DD HH:mm'
              });
            });
      </script>
    </div>
    <div class="col-md-4">
      <div class='col-sm-6'>
        <div class="form-group">
        
          <h6>Split By</h6>
          <select class="form-control selectpicker" id="selectSplitTime">
            <option custom="day">Day</option>
            <option custom="hour">Hour</option>
            <option custom="week">Week</option>
          </select>

        </div>
      </div>
    </div>

   
  </div>
</div>


  <button type="button" class="btn btn-info" onclick="fromTo()">Filter!</button>




<div id="tablediv">
  @{Html.RenderAction("Main", "Statistic");}
  </div>
