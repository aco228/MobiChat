@model List<MobiChat.Data.User>
 
<a class="btn btn-default" href='@Url.Action("CreateUser", "Users")'> Create User </a>

<table id="tableStat" class="table table-striped table table-sm" cellspacing="0" width="100%">
  <thead>
    <tr>
      <th>UserName</th>
      <th>Type</th>
      <th>Status</th>
      <th>Created</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>

    @foreach (var m in Model)
    {
      <tr>
        <td class="">@m.Username</td>
        <td class="">@m.UserType.Name</td>
        <td class="">@m.UserStatus</td>
        <td class="">@m.Created</td>
        <td class="">
          @Html.ActionLink("Edit", "EditUser", "Users", new { id= @m.ID}, new { @class = "btn btn-default glyphicon glyphicon-wrench" })  
        </td>

      </tr>
    }

  </tbody>
</table>


<div id="partialModal" style="margin-top:10px;margin-bottom:10px;">

</div>
